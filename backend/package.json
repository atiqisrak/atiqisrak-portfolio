{
    "name": "portfolio-backend",
    "version": "1.0.0",
    "description": "Backend API for portfolio website",
    "main": "dist/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/app.js",
        "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
        "test": "jest",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "migrate": "ts-node src/database/migrate.ts",
        "seed": "ts-node src/database/seed.ts"
    },
    "keywords": [
        "portfolio",
        "api",
        "express",
        "postgresql"
    ],
    "author": "Atiq Israk",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "pg": "^8.11.3",
        "dotenv": "^16.3.1",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "multer": "^1.4.5-lts.1",
        "sharp": "^0.32.6",
        "swagger-ui-express": "^5.0.0",
        "swagger-jsdoc": "^6.2.8"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/morgan": "^1.9.9",
        "@types/pg": "^8.10.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.0",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/swagger-jsdoc": "^6.0.4",
        "typescript": "^5.3.0",
        "ts-node": "^10.9.0",
        "ts-node-dev": "^2.0.0",
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.55.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0"
    }
}